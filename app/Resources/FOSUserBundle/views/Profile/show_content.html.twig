{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">

    <img src="{{ asset('photos/profiles/' ~ user.photo) }}" class="img-rounded" alt="Cinque Terre" width="304" height="236"/>

    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <p>Birthdate: {{ user.birthdate |date('Y-m-d') }}</p>
    <a href="{{ path('fos_user_profile_edit') }}">edit user !</a>
</div>
